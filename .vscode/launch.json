{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'redact-client'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=redact-client",
                    "--package=redact-client"
                ],
                "filter": {
                    "name": "redact-client",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {
                "REDACT_STORAGE_URL": "http://localhost:420",
                "REDACT_REDIS_CONN": "redis://127.0.0.1/",
                "REDACT_SERVER_PORT": "8080",
                "REDACT_CERTIFICATE_FILEPATH": "keys/client.pem",
            }
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'redact-client'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=redact-client",
                    "--package=redact-client"
                ],
                "filter": {
                    "name": "redact-client",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}